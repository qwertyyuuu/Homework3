<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/settings2">

    <fragment
        android:id="@+id/gallery"
        android:name="com.example.homework2.Gallery"
        android:label="Gallery"
        tools:layout="@layout/gallery_main"/>
    <fragment
        android:id="@+id/music"
        android:name="com.example.homework2.Music"
        android:label="Music"
        tools:layout="@layout/music_main"/>
    <fragment
        android:id="@+id/settings2"
        android:name="com.example.homework2.Settings"
        android:label="Settings"
        tools:layout="@layout/settings_main">
        <action
            android:id="@+id/action_settings2_to_gallery"
            app:destination="@id/gallery"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_settings2_to_notes"
            app:destination="@id/notes"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_settings2_to_music"
            app:destination="@id/music"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_settings2_to_message3"
            app:destination="@id/message3"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/notes"
        android:name="com.example.homework2.Notes"
        android:label="Notes"
        tools:layout="@layout/notes_main"/>
    <fragment
        android:id="@+id/message3"
        android:name="com.example.homework2.Message"
        android:label="Message"
        tools:layout="@layout/message_main"/>
</navigation>